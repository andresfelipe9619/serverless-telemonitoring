{
  "version": 1,
  "paths": {
    "/telemonitoring": {
      "name": "/telemonitoring",
      "lambdaFunction": "telemonitoringlambda",
      "permissions": {
        "groups": {
          "Doctors": [
            "create",
            "read",
            "update"
          ],
          "Patients": [
            "create",
            "read",
            "update"
          ]
        },
        "setting": "private"
      }
    },
    "/app": {
      "name": "/app",
      "lambdaFunction": "applambda",
      "permissions": {
        "groups": {
          "Doctors": [
            "create",
            "read",
            "update",
            "delete"
          ],
          "Patients": [
            "create",
            "read",
            "update"
          ]
        },
        "setting": "private"
      }
    }
  }
}